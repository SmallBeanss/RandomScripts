if not game:IsLoaded() then
	game.Loaded:Wait()
end
repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("FULLY_LOADED_CHAR")

local plr = game.Players.LocalPlayer 
Target = ""
Active = false

local Target = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local UIGradient = Instance.new("UIGradient")
local TargetInput = Instance.new("TextBox")
local Faceshot = Instance.new("ImageLabel")
local UICorner = Instance.new("UICorner")
local Submit = Instance.new("TextButton")
local Toggle = Instance.new("TextButton")
Target.Name = "Target"
Target.Parent = game.CoreGui
Target.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Main.Name = "Main"
Main.Parent = Target
Main.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
Main.BorderSizePixel = 0
Main.Position = UDim2.new(0.5, 0, 0.5, 0)
Main.Size = UDim2.new(0, 200, 0, 170)
Main.Active = true 
Main.Draggable = true
UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(0.19, Color3.fromRGB(234, 234, 234)), ColorSequenceKeypoint.new(0.45, Color3.fromRGB(212, 212, 212)), ColorSequenceKeypoint.new(0.84, Color3.fromRGB(191, 191, 191)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(166, 166, 166))}
UIGradient.Rotation = 90
UIGradient.Parent = Main
TargetInput.Name = "TargetInput"
TargetInput.Parent = Main
TargetInput.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
TargetInput.BorderSizePixel = 0
TargetInput.Position = UDim2.new(0.100000001, 0, 0.5, 0)
TargetInput.Size = UDim2.new(0, 160, 0, 30)
TargetInput.Font = Enum.Font.Gotham
TargetInput.Text = "sfsfs"
TargetInput.TextColor3 = Color3.fromRGB(245, 245, 245)
TargetInput.TextSize = 14.000
Faceshot.Name = "Faceshot"
Faceshot.Parent = Main
Faceshot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Faceshot.Position = UDim2.new(0.310000002, 0, 0.0299999993, 0)
Faceshot.Size = UDim2.new(0, 76, 0, 76)
Faceshot.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
UICorner.CornerRadius = UDim.new(1, 0)
UICorner.Parent = Faceshot
Submit.Name = "Submit"
Submit.Parent = Main
Submit.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Submit.BorderSizePixel = 0
Submit.Position = UDim2.new(0.200000003, 0, 0.764705896, 0)
Submit.Size = UDim2.new(0, 60, 0, 30)
Submit.Font = Enum.Font.GothamBold
Submit.Text = "Submit"
Submit.TextColor3 = Color3.fromRGB(255, 255, 255)
Submit.TextSize = 14.000
Toggle.Name = "Toggle"
Toggle.Parent = Main
Toggle.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Toggle.BorderSizePixel = 0
Toggle.Position = UDim2.new(0.5, 0, 0.764705896, 0)
Toggle.Size = UDim2.new(0, 60, 0, 30)
Toggle.Font = Enum.Font.GothamBold
Toggle.Text = "Enable"
Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
Toggle.TextSize = 14.000

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local tablefind = table.find
local MainEvent = ReplicatedStorage.MainEvent
local SpoofTable = { WalkSpeed = 16, JumpPower = 50 }
local Flags = { "CHECKER_1", "TeleportDetect", "OneMoreTime" }
local __namecall
__namecall = hookmetamethod(game, "__namecall", function(...)
	local args = {...}
	local self = args[1]
	local method = getnamecallmethod()
	local caller = getcallingscript()
	if (method == "FireServer" and self == MainEvent and tablefind(Flags, args[2])) then
		return
	end
	if (not checkcaller() and getfenv(2).crash) then
	hookfunction(getfenv(2).crash, function()
			warn("Crash Attempt") 
		end)
	end
	return __namecall(...)
end)
local __index
__index = hookmetamethod(game, "__index", function(t, k)
	if (not checkcaller() and t:IsA("Humanoid") and (k == "WalkSpeed" or k == "JumpPower")) then
		return SpoofTable[k]
	end
	return __index(t, k)
end)
local __newindex
__newindex = hookmetamethod(game, "__newindex", function(t, k, v)
	if (not checkcaller() and t:IsA("Humanoid") and (k == "WalkSpeed" or k == "JumpPower")) then
		SpoofTable[k] = v
		return
	end
	return __newindex(t, k, v)
end)

TargetInput.FocusLost:connect(function()
	for i,v in pairs(game.Players:GetChildren()) do
		if (string.sub(string.lower(v.Name),1,string.len(TargetInput.Text))) == string.lower(TargetInput.Text) then
			TargetInput.Text = v.Name
		end
	end
end)

TargetInput.FocusLost:Connect(function()
	wait(.1)
	local userId = game.Players:FindFirstChild(TargetInput.Text).UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = game.Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)	
	Faceshot.Image = content
end)

Submit.MouseButton1Click:Connect(function()
	wait(.11)
	if game.Players:FindFirstChild(TargetInput.Text) then Target = game.Players:FindFirstChild(TargetInput.Text)
		game.StarterGui:SetCore("SendNotification",{Title ='Target',Text ="Target changed to "..TargetInput.Text,Duration =15})
	else 
		game.StarterGui:SetCore("SendNotification",{Title ='Target',Text ="Error, No players with that name found",Duration =10})
	end
end)


local function Fists() 
	pcall(function()
		plr.Character:FindFirstChild("LeftHand").Size = Vector3.new(25,25,25)
		plr.Character:FindFirstChild("LeftHand").Massless = true 
		plr.Character:FindFirstChild("LeftHand").CanCollide = false 
		plr.Character:FindFirstChild("LeftHand").Transparency = .95 
		plr.Character:FindFirstChild("RightHand").Size = Vector3.new(25,25,25)
		plr.Character:FindFirstChild("RightHand").Massless = true 
		plr.Character:FindFirstChild("RightHand").CanCollide = false 
		plr.Character:FindFirstChild("RightHand").Transparency = .95
	end)
end

for i,a in pairs(game.Workspace:GetDescendants()) do 
	if a:IsA("Seat") then a:Destroy()
	end
end

game:GetService("RunService").RenderStepped:Connect(function()
	pcall(function()
		if game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild("K.O").Value == true then
			for i,z in pairs(plr.Character:GetChildren()) do 
				if z:IsA("Tool") then z.Parent = plr.Backpack
				end
			end
			game.Players.LocalPlayer.Character.Humanoid.Health = 0
			for nigger,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
				if v.ClassName == "Humanoid" or v.ClassName == "MeshPart" or v.ClassName == "Part" or v.ClassName == "Accessory"then
					v:Destroy() 
				end
			end
		end
	end)
end)

plr:GetMouse().KeyDown:Connect(function(key)
	if key == "v" then 
		if Main.Visible == true then Main.Visible = false else Main.Visible = true 
		end
	end	
end)

Toggle.MouseButton1Click:Connect(function()
	if Toggle.Text == "Enable" then Active = true Toggle.Text = "Disable" else 
		Active = false Toggle.Text = "Enable" plr.Character:FindFirstChild("HumanoidRootPart").CFrame = plr.Character:FindFirstChild("HumanoidRootPart").CFrame + Vector3.new(0,60,0)
		game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = plr.Character.Humanoid
		game:GetService("ReplicatedStorage").MainEvent:FireServer("Block",false)
	end
end)

local function Noclip() 
	pcall(function()
		for i,v in pairs(plr.Character:GetDescendants()) do 
			if v:IsA("BasePart") then v.CanCollide = false	
			end
		end
	end)
end

local function ViewTarget()
	pcall(function()
	game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = Target.Character.Head
	end)
end

game:GetService("RunService").RenderStepped:Connect(function()
	if Active == true and Target ~= "" and Target.Character then 
		if Target.Character.BodyEffects:FindFirstChild("K.O").Value == false and Target.Character.BodyEffects:FindFirstChild("Dead").Value == false then 
		if plr.Backpack:FindFirstChild("Combat") then plr.Backpack:FindFirstChild("Combat").Parent = plr.Character end 
			Fists()
			Noclip()
			ViewTarget()
			game:GetService("ReplicatedStorage").MainEvent:FireServer("Block",true)
		Pos = CFrame.new(Target.Character.LowerTorso.Position.X,Target.Character.LowerTorso.Position.Y,Target.Character.LowerTorso.Position.Z) + Vector3.new(0,-6.5,0)
		plr.Character.HumanoidRootPart.CFrame = Pos
		plr.Character:FindFirstChild("Combat"):Activate()
		else if Target.Character.BodyEffects:FindFirstChild("K.O").Value == true and Target.Character.BodyEffects:FindFirstChild("Dead").Value == false then
			Pos = CFrame.new(Target.Character.LowerTorso.Position.X,Target.Character.LowerTorso.Position.Y,Target.Character.LowerTorso.Position.Z) + Vector3.new(0,1.3,0)
				plr.Character.HumanoidRootPart.CFrame = Pos
				ViewTarget()
				game:GetService("ReplicatedStorage").MainEvent:FireServer("Block",true)
				game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
			else if Target.Character.BodyEffects:FindFirstChild("K.O").Value == true and Target.Character.BodyEffects:FindFirstChild("Dead").Value == true then
					Noclip()
					Pos = CFrame.new(Target.Character.LowerTorso.Position.X,Target.Character.LowerTorso.Position.Y,Target.Character.LowerTorso.Position.Z) + Vector3.new(0,-12,0)
					plr.Character.HumanoidRootPart.CFrame = Pos
					ViewTarget()
					game:GetService("ReplicatedStorage").MainEvent:FireServer("Block",true)
				end
			end
		end
	end
end)


